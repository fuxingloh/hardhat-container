{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "clean": {
      "cache": false
    },
    "build": {
      "inputs": ["tsconfig.json", "tsconfig.build.json", "./**/*.ts", "./**/*.json"],
      "outputs": ["./**/*.js", "./**/*.d.ts", "./**/*.d.ts.map"],
      "dependsOn": ["^build"]
    },
    "build:docker": {
      "cache": false,
      "inputs": ["Dockerfile", "tsconfig.json", "tsconfig.build.json", "./**/*.ts"],
      "outputs": []
    },
    "push:docker": {
      "cache": false
    },
    "test": {
      "inputs": ["tsconfig.json", "./**/*.ts", "./**/*.test.ts"],
      "dependsOn": ["^build"]
    },
    "lint": {
      "outputs": []
    }
  }
}
